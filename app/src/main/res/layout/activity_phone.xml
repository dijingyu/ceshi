<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.dijingyu.app_demo.ui.PhoneActivity">

    <ImageView
        android:id="@+id/iv_back"
        android:src="@mipmap/back_white"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_35"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_width="@dimen/dp_25"
        android:layout_height="@dimen/dp_25"
        tools:ignore="ContentDescription" />
    <TextView
        android:id="@+id/tv_time"
        android:enabled="false"
        android:layout_marginLeft="@dimen/dp_252"
        android:layout_marginStart="@dimen/dp_252"
        android:layout_marginTop="@dimen/dp_37"
        android:layout_width="@dimen/dp_108"
        android:textSize="@dimen/sp_14"
        android:textColor="#999999"
        android:layout_height="@dimen/dp_20" />

    <TextView
        android:layout_width="@dimen/dp_170"
        android:layout_height="@dimen/dp_38"
        android:textColor="#373737"
        android:textSize="@dimen/sp_27"
        android:text="@string/verify"
        android:layout_marginTop="@dimen/dp_104"
        android:layout_marginLeft="@dimen/dp_23"
        android:layout_marginStart="@dimen/dp_23" />

    <android.support.constraint.ConstraintLayout
        android:background="@drawable/bg_edittext"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_56"
        android:layout_marginTop="@dimen/dp_255"
        android:layout_marginLeft="@dimen/dp_55"
        android:layout_marginStart="@dimen/dp_55" >

        <EditText
            android:inputType="number"
            android:id="@+id/ed_one"
            style="?android:attr/textViewStyle"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_45"
            android:background="@null"
            android:maxLength="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:textColor="#636363"
            android:textSize="@dimen/sp_32"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"
            tools:ignore="MissingConstraints" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:background="@drawable/bg_edittext"
        android:textSize="@dimen/sp_32"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_56"
        android:layout_marginTop="@dimen/dp_255"
        android:layout_marginLeft="@dimen/dp_129"
        android:layout_marginStart="@dimen/dp_129" >
        <EditText
            android:inputType="number"
            android:id="@+id/ed_two"
            android:maxLength="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:textColor="#636363"
            android:textSize="@dimen/sp_32"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_45" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:background="@drawable/bg_edittext"
        android:textSize="@dimen/sp_32"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_56"
        android:layout_marginTop="@dimen/dp_255"
        android:layout_marginLeft="@dimen/dp_203"
        android:layout_marginStart="@dimen/dp_203" >
        <EditText
            android:inputType="number"
            android:maxLength="1"
            android:id="@+id/ed_three"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:textColor="#636363"
            android:textSize="@dimen/sp_32"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_45" />
    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:background="@drawable/bg_edittext"
        android:textSize="@dimen/sp_32"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_56"
        android:layout_marginTop="@dimen/dp_255"
        android:layout_marginLeft="@dimen/dp_277"
        android:layout_marginStart="@dimen/dp_277" >
        <EditText
            android:inputType="number"
            android:id="@+id/ed_four"
            android:maxLength="1"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:textColor="#636363"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:textSize="@dimen/sp_32"
            android:layout_width="@dimen/dp_21"
            android:layout_height="@dimen/dp_45" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/tv_tijiao"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_20"
        android:textColor="#FA6A13"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_314"
        android:layout_marginLeft="@dimen/dp_158"
        android:layout_marginStart="@dimen/dp_158" />

</RelativeLayout>
