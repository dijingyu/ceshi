<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#111111"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dijingyu.app_demo.ui.MainActivity">

    <android.support.v7.widget.Toolbar
        android:layout_marginTop="@dimen/dp_22"
        android:id="@+id/toolbar"
        android:background="#17181a"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_62"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:text="@string/my"
            android:textColor="#fff"
            android:textSize="@dimen/sp_19"
            android:id="@+id/textView2" />
        <ImageView
            android:src="@mipmap/message_default"
            android:layout_width="@dimen/dp_22"
            android:layout_height="@dimen/dp_16"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="7dp"
            android:layout_marginRight="@dimen/dp_17"
            android:layout_marginBottom="@dimen/dp_16"
            android:id="@+id/imageView"
            tools:ignore="ContentDescription" />

            <ImageView
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="@dimen/dp_28"
                android:layout_marginRight="@dimen/dp_3"
                android:src="@mipmap/message_spot_default"
                tools:ignore="ContentDescription"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/dp_3" />
            
        </RelativeLayout>
    </android.support.v7.widget.Toolbar>

    <RelativeLayout
        android:layout_below="@id/toolbar"
        android:background="@mipmap/my_account_bg_default"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_100"
        android:id="@+id/personage_relative">
        <ImageView
            android:id="@+id/iv_tou"
            android:src="@mipmap/di"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="@dimen/dp_70"
            android:layout_height="@dimen/dp_70"
            android:layout_marginStart="@dimen/dp_15"
            tools:ignore="ContentDescription" />

        <TextView
            android:layout_marginTop="@dimen/dp_20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_toEndOf="@+id/iv_tou"
            android:layout_toRightOf="@+id/iv_tou"
            android:text="@string/name"
            android:textColor="#fff"
            android:textSize="@dimen/sp_20"
            android:id="@+id/tv_name"
            android:layout_marginStart="@dimen/dp_15"
            tools:ignore="RelativeOverlap" />

        <TextView
            android:layout_marginTop="@dimen/dp_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/tv_name"
            android:layout_alignStart="@+id/tv_name"
            android:layout_below="@+id/tv_name"
            android:text="@string/title"
            android:textColor="#bbb"
            android:textSize="@dimen/sp_12" />

        <ImageView
            android:src="@mipmap/big_arrow_right"
            android:layout_width="@dimen/dp_13"
            android:layout_height="@dimen/dp_22"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_30"
            android:layout_marginEnd="@dimen/dp_30"
            tools:ignore="ContentDescription" />
        
    </RelativeLayout>

    <RelativeLayout
        android:background="@mipmap/bg_number_default"
        android:layout_margin="@dimen/dp_15"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_140"
        android:layout_below="@+id/personage_relative"
        android:id="@+id/relativeLayout1">
        <TextView
            android:text="我的米粒"
            android:layout_margin="@dimen/dp_5"
            android:textColor="#f1f1f1"
            android:textSize="@dimen/sp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="HardcodedText" />
        <TextView
            android:textColor="#fff"
            android:text="875"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center"
            android:textSize="@dimen/sp_65"
            android:id="@+id/textView3"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignBottom="@+id/textView3"
            android:layout_toEndOf="@+id/textView3"
            android:layout_toRightOf="@+id/textView3"
            android:text="米粒"
            android:textColor="#fff"
            android:textSize="@dimen/sp_15"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:text="查看详情>>"
            android:layout_margin="@dimen/dp_5"
            android:textColor="#f1f1f1"
            android:textSize="@dimen/sp_18"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <RelativeLayout
        android:background="@drawable/bg_border"
        android:layout_width="@dimen/dp_340"
        android:layout_height="@dimen/dp_240"
        android:layout_margin="@dimen/dp_17"
        android:layout_gravity="center"
        android:layout_below="@+id/relativeLayout1"
        android:id="@+id/relativeLayout2">

        <View
            android:id="@+id/view"
            android:layout_height="@dimen/dp_240"
            android:layout_width="@dimen/dp_1"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:background="#4d4d4d"
            android:layout_gravity="center"/>

        <RelativeLayout
            android:layout_width="@dimen/dp_170"
            android:layout_height="@dimen/dp_80"
            android:layout_alignEnd="@+id/view"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/view"
            android:id="@+id/answers_relative">
            <TextView
                android:text="@string/answers"
                android:textColor="#fff"
                android:textSize="@dimen/sp_17"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_answer"
                tools:ignore="RelativeOverlap"
                android:layout_marginStart="@dimen/dp_5" />

            <ImageView
                android:src="@mipmap/my_question_disabled"
                android:layout_width="@dimen/dp_18"
                android:layout_height="@dimen/dp_18"
                android:layout_alignBottom="@+id/tv_answer"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                tools:ignore="ContentDescription" />

            <ImageView
                android:src="@mipmap/rectangular_dividing_line_disabled"
                android:layout_width="@dimen/dp_26"
                android:layout_height="@dimen/dp_3"
                android:layout_alignLeft="@+id/tv_answer"
                android:layout_alignStart="@+id/tv_answer"
                android:layout_below="@+id/tv_answer"
                android:layout_marginTop="15dp"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="@dimen/dp_170"
            android:layout_height="@dimen/dp_80"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/view"
            android:layout_toRightOf="@+id/view"
            android:id="@+id/collect_relative">
            <TextView
                android:id="@+id/tv_collect"
                android:text="@string/collect"
                android:textColor="#fff"
                android:textSize="@dimen/sp_17"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="RelativeOverlap"
                android:layout_marginStart="@dimen/dp_5" />

            <ImageView
                android:src="@mipmap/collection_icons"
                android:layout_width="@dimen/dp_18"
                android:layout_height="@dimen/dp_18"
                android:layout_alignBottom="@+id/tv_collect"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                tools:ignore="ContentDescription" />
            <ImageView
                android:src="@mipmap/rectangular_dividing_line_disabled"
                android:layout_width="@dimen/dp_26"
                android:layout_height="@dimen/dp_3"
                android:layout_alignLeft="@+id/tv_collect"
                android:layout_alignStart="@+id/tv_collect"
                android:layout_below="@+id/tv_collect"
                android:layout_marginTop="15dp"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/journey_relative"
            android:layout_width="@dimen/dp_170"
            android:layout_height="@dimen/dp_80"
            android:layout_alignEnd="@+id/view"
            android:layout_alignRight="@+id/view"
            android:layout_below="@+id/answers_relative">
            <TextView
                android:id="@+id/tv_journey"
                android:text="@string/journey"
                android:textColor="#fff"
                android:textSize="@dimen/sp_17"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="RelativeOverlap"
                android:layout_marginStart="@dimen/dp_5" />

            <ImageView
                android:src="@mipmap/buy_journey_icons"
                android:layout_width="@dimen/dp_18"
                android:layout_height="@dimen/dp_18"
                android:layout_alignBottom="@+id/tv_journey"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                tools:ignore="ContentDescription" />

            <ImageView
                android:src="@mipmap/rectangular_dividing_line_disabled"
                android:layout_width="@dimen/dp_26"
                android:layout_height="@dimen/dp_3"
                android:layout_alignLeft="@+id/tv_journey"
                android:layout_alignStart="@+id/tv_journey"
                android:layout_below="@+id/tv_journey"
                android:layout_marginTop="15dp"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/service_relative"
            android:layout_width="@dimen/dp_170"
            android:layout_height="@dimen/dp_80"
            android:layout_below="@+id/collect_relative"
            android:layout_toEndOf="@+id/view"
            android:layout_toRightOf="@+id/view">

            <TextView
                android:id="@+id/tv_service"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_14"
                android:text="@string/service"
                android:textColor="#fff"
                android:textSize="@dimen/sp_17"
                android:layout_marginStart="@dimen/dp_5"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:layout_width="@dimen/dp_18"
                android:layout_height="@dimen/dp_18"
                android:layout_alignBottom="@+id/tv_service"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:src="@mipmap/add_friend_icon"
                tools:ignore="ContentDescription" />
            <ImageView
                android:src="@mipmap/rectangular_dividing_line_disabled"
                android:layout_width="@dimen/dp_26"
                android:layout_height="@dimen/dp_3"
                android:layout_alignLeft="@+id/tv_service"
                android:layout_alignStart="@+id/tv_service"
                android:layout_below="@+id/tv_service"
                android:layout_marginTop="15dp"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/versions_relative"
            android:layout_width="@dimen/dp_170"
            android:layout_height="@dimen/dp_80"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/speak_relative">

            <TextView
                android:id="@+id/tv_versions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_14"
                android:text="@string/versions"
                android:textColor="#fff"
                android:textSize="@dimen/sp_17"
                tools:ignore="RelativeOverlap"
                android:layout_marginStart="@dimen/dp_5" />

            <ImageView
                android:layout_width="@dimen/dp_18"
                android:layout_height="@dimen/dp_18"
                android:layout_alignBottom="@+id/tv_versions"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:src="@mipmap/new_edition_icons"
                tools:ignore="ContentDescription" />

            <ImageView
                android:src="@mipmap/rectangular_dividing_line_disabled"
                android:layout_width="@dimen/dp_26"
                android:layout_height="@dimen/dp_3"
                android:layout_alignLeft="@+id/tv_versions"
                android:layout_alignStart="@+id/tv_versions"
                android:layout_below="@+id/tv_versions"
                android:layout_marginTop="15dp"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/speak_relative"
            android:layout_width="@dimen/dp_170"
            android:layout_height="@dimen/dp_80"
            android:layout_alignLeft="@+id/view"
            android:layout_alignStart="@+id/view"
            android:layout_below="@+id/journey_relative">

            <TextView
                android:id="@+id/tv_speak"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_14"
                android:text="@string/speak"
                android:textColor="#fff"
                android:textSize="@dimen/sp_17"
                tools:ignore="RelativeOverlap"
                android:layout_marginStart="@dimen/dp_5" />

            <ImageView
                android:src="@mipmap/comment_icons"
                android:layout_width="@dimen/dp_18"
                android:layout_height="@dimen/dp_18"
                android:layout_alignBottom="@+id/tv_speak"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                tools:ignore="ContentDescription" />

            <ImageView
                android:src="@mipmap/rectangular_dividing_line_disabled"
                android:layout_width="@dimen/dp_26"
                android:layout_height="@dimen/dp_3"
                android:layout_alignLeft="@+id/tv_speak"
                android:layout_alignStart="@+id/tv_speak"
                android:layout_below="@+id/tv_speak"
                android:layout_marginTop="15dp"
                tools:ignore="ContentDescription" />

        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>